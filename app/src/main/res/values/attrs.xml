<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleBatteryView">
        <!-- 底层圈color -->
        <attr name="_backColor" format="color" />
        <!-- 上层圈color -->
        <attr name="_foreColor" format="color" />
        <!-- 底层圈线宽 -->
        <attr name="_backStrokeWidth" format="dimension" />
        <!-- 上层圈线宽 -->
        <attr name="_foreStrokeWidth" format="dimension" />
        <!-- 是否显示电池电量文本 -->
        <attr name="_showBatteryText" format="boolean" />
        <!-- 电量文本字体大小 -->
        <attr name="_batteryTextSize" format="dimension" />
        <!-- 电池电量文本颜色 -->
        <attr name="_batteryTextColor" format="color" />
        <!-- 电池电量 -->
        <attr name="_batteryLevel" format="integer" />
    </declare-styleable>
</resources>